{
  "N8n": {
    "Host": "localhost",
    "Port": 5678,
    "UseHttps": false,
    "ApiKey": "",
    "ProcessPath": "n8n",
    "LogPath": "~/n8n.log",
    "StartupTimeoutSeconds": 30
  },
  "Claude": {
    "Command": "claude",
    "Arguments": ["--print"],
    "TimeoutSeconds": 60
  },
  "Templates": {
    "Directory": "../../templates",
    "EmbeddedResourceNamespace": "FlowForge.Console.Templates"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "FlowForge": "Debug",
      "Microsoft.Hosting.Lifetime": "Information",
      "System.Net.Http.HttpClient": "Warning"
    }
  }
}